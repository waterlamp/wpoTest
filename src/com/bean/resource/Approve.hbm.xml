<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bean.Approve" table="approves" schema="wpobase">
        <id name="approveId" column="approve_id"/>
        <property name="approveOpinion" column="approve_opinion"/>
        <property name="approveStartTime" column="approve_start_time"/>
        <property name="approveEndTime" column="approve_end_time"/>
    </class>
</hibernate-mapping>