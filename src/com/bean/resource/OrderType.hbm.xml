<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bean.OrderType" table="ordertypes" schema="wpobase">
        <id name="ordertypeId" column="ordertype_id">
            <generator class="native"/>
        </id>
        <property name="ordertypeName" column="ordertype_name"/>
    </class>
</hibernate-mapping>